<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.sigil.fantasyradio"
    android:installLocation="auto"
    android:versionCode="123"
    android:versionName="1.23
    " >

    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <permission android:name="ru.sigil.fantasyradio.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="ru.sigil.fantasyradio.permission.C2D_MESSAGE" />


    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="20" />

    <application
        android:name="MyApp"
        android:configChanges="orientation|screenSize"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Black.NoTitleBar" >
        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".archieve.ArchieveActivity"
            android:configChanges="orientation"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".saved.SavedActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".schedule.ScheduleActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".settings.SettingsActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main" >
        </activity>
        <activity android:name="com.lamerman.FileDialog" >
        </activity>
        <activity
            android:name=".TabHoster"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".About"
            android:label="@string/about"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name=".Gratitude"
            android:label="@string/gratitude"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"/>
        <receiver android:name=".utils.AlarmReceiever" />
        <receiver
            android:name=".gcm.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.example.gcm" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.google.android.gms.analytics.CampaignTrackingReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>
        <!--<receiver android:name=".widget.FantasyRadioWidgetProvider" >
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>
            <meta-data android:name="android.appwidget.provider"
                android:resource="@xml/example_appwidget_info" />
        </receiver>-->
        <service android:name=".gcm.GcmIntentService" />
        <service android:name="com.google.android.gms.analytics.CampaignTrackingService" />
    </application>
</manifest>